{
    "name": "matthewbdaly/laravel-golden-master-tests",
    "description": "Package for writing golden master tests",
    "type": "library",
    "keywords": ["laravel","lumen"],
    "require": {
        "laravel/browser-kit-testing": "^5.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "matthewbdaly@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Matthewbdaly\\LaravelGoldenMasterTests\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4"
    }
}
